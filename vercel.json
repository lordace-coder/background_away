{
  "version": 2,
  "builds": [
    {
      "src": "flask_app/main.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements.txt",
        "excludeFiles": ["**/*.pyc", "**/__pycache__/*", "tests/*", "*.log"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "flask_app/main.py"
    }
  ]
}
